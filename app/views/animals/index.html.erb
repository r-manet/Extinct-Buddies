              <% else %>
                <% image_tag 'data:image/jpeg;base64,' %>
              <% end %>
              <div class="card-body"style="background-color: white; height: 150px;">
                <div class="d-flex justify-content-between align-items-center-mb-2">
                    <h5 class="card-title pt-0 mb-0"> <%= animal.name %></h5><span>Â£<%= animal.price %></span>
                </div>
                <p class="card-text"><%= animal.description%></p>
                <%= link_to 'Buddy details', animal_path(animal), :class =>"btn btn-dark"%>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>
 </div>



