<div class="container">
  <div class="row">
    <div class="col-md-6">
      <div class="animal-grid">
        <% @animals.each do |animal| %>
          <div class="animal-card-wrapper" style : {width: 260px}>
            <div class="animal-card">
              <% if animal.photo.attached? %>
